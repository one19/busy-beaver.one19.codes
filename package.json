{
  "name": "busy-beaver",
  "version": "0.0.0",
  "description": "frontend",
  "main": "index.html",
  "scripts": {
    "precommit": "yarn build && lint-staged",
    "start": "parcel index.html",
    "clean": "rm -rf docs/*.js docs/*.html docs/*.map dist coverage .cache .nyc_output",
    "prebuild": "yarn clean",
    "build": "parcel build index.html --public-url https://one19.github.io/busy-beaver/",
    "prod": "(cd docs && python -m SimpleHTTPServer 8000)",
    "postbuild": "mkdir docs || true && cp dist/* docs/ && rm -rf docs/*.map",
    "test": "ava",
    "lint": "eslint . --fix",
    "coverage": "nyc ava",
    "check-deps": "npm i -g npm-check && npm-check -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/one19/busy-beaver.git"
  },
  "keywords": [
    "frontend",
    "react"
  ],
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/one19/busy-beaver/issues"
  },
  "homepage": "https://github.com/one19/busy-beaver#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@brightsole/eslint-config-lint-front": "0.0.6",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-info": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "a-plus-forms": "^0.9.2",
    "a-plus-forms-json-validator": "^1.0.6",
    "ava": "^2.2.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "chroma-js": "^2.0.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "husky": "^1.1.2",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.2.1",
    "localforage": "^1.7.2",
    "nanoid": "^2.0.3",
    "nock": "^10.0.1",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.10.3",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^4.0.3"
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "react-chartjs-2": "^2.7.6"
  }
}
